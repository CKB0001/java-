<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
  <servlet>
    <servlet-name>Questionnaire</servlet-name>
    <servlet-class>cc.openhome.Questionnaire</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>Login</servlet-name>
    <servlet-class>cc.openhome.Login</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>User</servlet-name>
    <servlet-class>cc.openhome.User</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>Logout</servlet-name>
    <servlet-class>cc.openhome.Logout</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>Count</servlet-name>
    <servlet-class>cc.openhome.Count</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Questionnaire</servlet-name>
    <url-pattern>/question.do</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>Login</servlet-name>
    <url-pattern>/login.do</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>User</servlet-name>
    <url-pattern>/user.view</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>Logout</servlet-name>
    <url-pattern>/logout.do</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>Count</servlet-name>
    <url-pattern>/count.do</url-pattern>
  </servlet-mapping>
  <session-config>
    <session-timeout>30</session-timeout>
  </session-config>
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <description></description>
    <display-name>ListBookServelt</display-name>
    <servlet-name>ListBookServelt</servlet-name>
    <servlet-class>com.servelt.ListBookServelt</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ListBookServelt</servlet-name>
    <url-pattern>/ListBookServelt</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>AddshoppingServelt</display-name>
    <servlet-name>AddshoppingServelt</servlet-name>
    <servlet-class>com.servelt.AddshoppingServelt</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>AddshoppingServelt</servlet-name>
    <url-pattern>/AddshoppingServelt</url-pattern>
  </servlet-mapping>
</web-app>